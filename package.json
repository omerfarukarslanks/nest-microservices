{
  "name": "training-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "//LOCAL": "LOCAL",
    "start": "dotenv -e env/local.env -- nx run-many --target=serve --parallel=4",
    "start:api-gateway": "dotenv -e env/local.env -- nx serve api-gateway",
    "start:customer-service": "dotenv -e env/local.env -- nx serve customer-service",
    "start:uaa-service": "dotenv -e env/local.env -- nx serve uaa-service",
    "start:user-service": "dotenv -e env/local.env -- nx serve user-service",
    "migrate": "dotenv -e env/local.env -- prisma migrate dev --preview-feature",
    "migrate:create": "dotenv -e env/local.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:deploy": "dotenv -e env/local.env -- prisma migrate deploy --preview-feature",
    "migrate:pull": "dotenv -e env/local.env -- prisma db pull",
    "migrate:reset": "dotenv -e env/local.env -- prisma migrate reset --preview-feature",
    "//TEST": "TEST",
    "start:test": "dotenv -e env/test.env -- nx run-many --target=serve --parallel=4",
    "start:api-gateway:test": "dotenv -e env/test.env -- nx serve api-gateway",
    "start:customer-service:test": "dotenv -e env/test.env -- nx serve customer-service",
    "start:uaa-service:test": "dotenv -e env/test.env -- nx serve uaa-service",
    "start:user-service:test": "dotenv -e env/test.env -- nx serve user-service",
    "migrate:test": "dotenv -e env/test.env -- prisma migrate dev --preview-feature",
    "migrate:test:create": "dotenv -e env/test.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:test:deploy": "dotenv -e env/test.env -- prisma migrate deploy --preview-feature",
    "migrate:test:pull": "dotenv -e env/test.env -- prisma db pull",
    "migrate:test:reset": "dotenv -e env/test.env -- prisma migrate reset --preview-feature",
    "//PROD": "PROD",
    "start:prod": "dotenv -e env/production.env -- node dist/src/main",
    "start:api-gateway:prod": "dotenv -e env/production.env -- nx serve api-gateway",
    "start:customer-service:prod": "dotenv -e env/production.env -- nx serve customer-service",
    "start:uaa-service:prod": "dotenv -e env/production.env -- nx serve uaa-service",
    "start:user-service:prod": "dotenv -e env/production.env -- nx serve user-service",
    "migrate:prod": "dotenv -e env/production.env -- prisma migrate dev --preview-feature",
    "migrate:prod:create": "dotenv -e env/production.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:prod:deploy": "dotenv -e env/production.env -- prisma migrate deploy --preview-feature",
    "migrate:prod:pull": "dotenv -e env/production.env -- prisma db pull",
    "migrate:prod:reset": "dotenv -e env/production.env -- prisma migrate reset --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "postinstall": "npm run prisma:generate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.10.1",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "helmet": "^6.0.1",
    "http-proxy-middleware": "^2.0.6",
    "kafkajs": "^2.2.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.8.6",
    "@nrwl/jest": "15.8.6",
    "@nrwl/linter": "15.8.6",
    "@nrwl/nest": "15.8.6",
    "@nrwl/node": "15.8.6",
    "@nrwl/nx-cloud": "15.2.1",
    "@nrwl/webpack": "15.8.6",
    "@nrwl/workspace": "15.8.6",
    "@types/jest": "29.4.1",
    "@types/node": "^18.7.1",
    "@types/passport-jwt": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "nx": "15.8.6",
    "prettier": "^2.6.2",
    "prisma": "^4.10.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
