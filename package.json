{
  "name": "training-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e env/local.env -- nx run-many --target=serve",
    "start:dev": "dotenv -e env/local.env -- nest start --watch",
    "start:debug": "dotenv -e env/local.env -- nest start --debug --watch",
    "start:prod": "dotenv -e env/production.env -- node dist/src/main",
    "migrate:dev": "dotenv -e env/local.env -- prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e env/local.env -- prisma migrate dev --create-only --preview-feature",
    "migrate:dev:deploy": "dotenv -e env/local.env -- prisma migrate deploy --preview-feature",
    "migrate:dev:pull": "dotenv -e env/local.env -- prisma db pull",
    "migrate:reset": "dotenv -e env/local.env -- prisma migrate reset --preview-feature",
    "migrate:deploy:prod": "dotenv -e env/production.env -- npx prisma migrate deploy --preview-feature",
    "migrate:deploy:dev": "dotenv -e env/dev.env -- npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "postinstall": "npm run prisma:generate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.10.1",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "helmet": "^6.0.1",
    "http-proxy-middleware": "^2.0.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.7.1",
    "@nrwl/jest": "15.7.1",
    "@nrwl/linter": "15.7.1",
    "@nrwl/nest": "15.7.1",
    "@nrwl/node": "15.7.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/webpack": "15.7.1",
    "@nrwl/workspace": "15.7.1",
    "@types/jest": "28.1.1",
    "@types/node": "^18.7.1",
    "@types/passport-jwt": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "15.7.1",
    "prettier": "^2.6.2",
    "prisma": "^4.10.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "ts-proto": "^1.139.0",
    "typescript": "~4.8.2"
  }
}
